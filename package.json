{
  "name": "sveltekit-sanity-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "build:sanity": "cd studio && npm run build",
    "start:sanity": "cd studio && npm run dev",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "clean-install": "rmdir node_modules /s && npm install"
  },
  "devDependencies": {
    "@portabletext/svelte": "^0.2.0",
    "@sveltejs/adapter-netlify": "^1.0.0-next.36",
    "@sveltejs/kit": "1.0.0-next.202",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "svelte": "^3.42.6",
    "svelte-image-gallery": "^1.5.0",
    "svelte-lightbox": "^0.6.1"
  },
  "type": "module",
  "dependencies": {
    "@mdi/js": "^6.5.95",
    "@sanity/client": "^2.23.0",
    "@sanity/image-url": "^1.0.1",
    "svelte-image": "^0.2.9",
    "svelte-material-icons": "^1.0.3",
    "svelte-material-ui": "^6.0.0-beta.13",
    "svelte-materialify": "^0.3.11"
  }
}
